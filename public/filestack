<script type="text/javascript">
  var fsClient = filestack.init('AbXZQKrBT1SK0QR1sRkQtz');
  function openPicker() {
    fsClient.pick({
      accept: 'image/*',
      minFiles: 1,
      maxFiles: 1,
      maxSize: (100 * 1024 * 1024)
    }).then(function(response) {
      // declare this function to handle response
      handleFilestack(response);
    });
  }
</script>

